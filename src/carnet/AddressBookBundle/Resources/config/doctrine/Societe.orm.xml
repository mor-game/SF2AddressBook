<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="carnet\AddressBookBundle\Entity\Societe" table="societe">
    <id name="idSociete" type="integer" column="id_societe">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="80" nullable="false"/>
    <field name="adresse" type="text" column="adresse" nullable="false"/>
    <field name="ville" type="string" column="ville" length="80" nullable="false"/>
    <field name="cp" type="string" column="cp" length="5" nullable="false"/>
  </entity>
</doctrine-mapping>
